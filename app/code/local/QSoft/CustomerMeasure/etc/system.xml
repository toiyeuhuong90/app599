<?xml version="1.0" ?>
<config>
	<tabs>
		<qsoft translate="label" module="qsoft_customermeasure">
			<label>
				<![CDATA[
                <div style="padding-left:22px; background: url(data:image/png;base64,) left 50% no-repeat;">QSoft Extensions</div> ]]>
			</label>
			<sort_order>100</sort_order>
		</qsoft>
	</tabs>
	<sections>
		<customer_measure translate="label" module="qsoft_customermeasure">
			<label>Customer Measure Settings</label>
			<sort_order>2</sort_order>
			<class>superclass</class>
			<show_in_default>1</show_in_default>
			<show_in_website>1</show_in_website>
			<show_in_store>1</show_in_store>
			<tab>qsoft</tab>
			<groups>

                <files translate="label" module="qsoft_customermeasure">
					<label>Settings</label>
					<frontend_type>text</frontend_type>
					<sort_order>100</sort_order>
					<show_in_default>1</show_in_default>
					<show_in_website>1</show_in_website>
					<show_in_store>1</show_in_store>
					<fields>
						<default_weight translate="label" module="qsoft_customermeasure">
							<label>Unit default for Weight </label>
							<frontend_type>select</frontend_type>
							<source_model>qsoft_customermeasure/source_weight</source_model>
							<sort_order>1</sort_order>
							<show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
						</default_weight>
						<default_height translate="label" module="qsoft_customermeasure">
							<label>Unit default for Height</label>
							<frontend_type>select</frontend_type>
							<source_model>qsoft_customermeasure/source_height</source_model>
							<sort_order>3</sort_order>
							<show_in_default>1</show_in_default>
							<show_in_website>1</show_in_website>
							<show_in_store>1</show_in_store>
						</default_height>
                    </fields>
				</files>
			</groups>
		</customer_measure>
	</sections>
</config>